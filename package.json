{
  "name": "check",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "nodemon --ignore tmp/ --ignore node_modules/ --ignore public/ --ignore app/assets/ app.js",
    "start": "forever app.js",
    "test": "mocha",
    "build-templates": "./scripts/build-templates",
    "build-js": "./scripts/build-js",
    "build-css": "./scripts/build-css",
    "postinstall": "npm run build-templates && broccoli build assets"
  },
  "dependencies": {
    "express": "3.4.4",
    "express3-handlebars": "~0.5.0",
    "node-sass": "~0.8.0",
    "clean-css": "~2.0.6",
    "uglify-js": "~2.4.8",
    "require-directory": "~1.2.0",
    "handlebars": "~1.3.0",
    "ember-precompile": "~0.2.4",
    "mongoose": "~3.8.4",
    "forever": "~0.10.11",
    "uglifyjs": "~2.3.6",
    "node-require-directory": "~1.0.2",
    "jquery-replace-class": "0.0.1",
    "nodemon": "~1.0.13",
    "mongoose-validator": "~0.2.2",
    "mongoose-timestamp": "~0.1.1",
    "mongoose-unique-validator": "~0.3.0",
    "passport": "~0.2.0",
    "mongoose-findorcreate": "~0.1.2",
    "connect-mongo": "~0.4.0",
    "mongoose-gravatar": "~0.2.1",
    "marked": "~0.3.0",
    "highlight.js": "~8.0.0",
    "underscore": "~1.5.2",
    "async": "~0.2.10",
    "passport-github": "^0.1.5",
    "broccoli": "^0.2.4",
    "broccoli-sass": "^0.1.2",
    "broccoli-uglify-js": "^0.1.0",
    "broccoli-clean-css": "^0.1.3",
    "broccoli-cli": "0.0.1",
    "browserify": "^3.32.1",
    "browserify-shim": "^3.3.1"
  },
  "devDependencies": {
    "node-pow": "0.0.2",
    "browserify-middleware": "~2.2.0",
    "gulp": "~3.3.2",
    "gulp-util": "~2.2.9",
    "mocha": "~1.17.1",
    "chai": "~1.9.0",
    "broccoli-browserify": "0.0.1"
  },
  "engines": {
    "node": "0.10.x",
    "npm": "1.3.x"
  },
  "browser": {
    "jquery": "./app/assets/js/vendor/jquery.js",
    "fastClick": "./app/assets/js/vendor/fastclick.js",
    "Ember": "./app/assets/js/vendor/ember.js",
    "jquery.transit": "./app/assets/js/vendor/jquery.transit.js",
    "Modernizr": "./app/assets/js/vendor/modernizr.js"
  },
  "browserify-shim": {
    "jquery": {
      "exports": "$"
    },
    "jquery.transit": {
      "depends": [
        "jquery"
      ]
    },
    "fastClick": {
      "exports": "FastClick"
    },
    "Modernizr": {
      "exports": "Mondernizr"
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  }
}
